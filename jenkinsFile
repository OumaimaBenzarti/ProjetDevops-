pipeline {
    agent any
    
    stages{
        stage('Tools install'){
            steps{
                
                echo 'Tools'
            }
            
        }
        
        stage('Geeting project from git'){
            steps{
                  echo 'Récupérer le projet à partir de git';
                    git branch: 'nourchene',
					url : 'https://github.com/OumaimaBenzarti/ProjetDevops-.git';
            }
            
        }
        
        stage('Cleaning the projects'){
            steps{
                
                  
  	echo 'Testing maven'
	sh " mvn -version"
	sh "mvn help:effective-settings"
	sh "mvn clean"
	
	
		
		
            }
            
        }
        
        stage('Unit Tests (pas encore )'){
            steps{
                
                echo 'Test Unitaire'
            }
            
        }
        
        stage('Code Quality Check via SONARQUBE  (pas encore ) '){
            steps{
                
                echo 'Analyse le code source avec SONARQUBE'
		
            }
            
        }
        
         stage('Publish To Nexus (pas encore )'){
            steps{
                
                echo 'publier à nexus'
            }
            
        }
    
        
         stage('Building our image (pas encore )'){
            steps{
                
                echo 'Building our image'
            }
            
        }
        
          stage('Deploy  our image (pas encore ) '){
            steps{
                
                echo 'Déploiement de notre image'
            }
            
        }
        
        
    }
    
}
